<thead>
  <tr>
    <th>検索</th>
  </tr>
</thead>
<%= form_with url: users_path, local: true do |f| %>
  <%= f.text_field :user_name %>
  <%= f.submit :検索 %>
<% end %>
<table class="search-form">
  <tbody>
    <% @user.user_name.each do |user| %>
      <tr>
        <td>
          <%= link_to user.user_name, user_path(user.id) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>